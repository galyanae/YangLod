<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.goodthinking.youngloduser.user">

    <!-- To auto-complete the email text field in the login form with the youngloduser's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon1"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name="android.support.multidex.MultiDexApplication"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.goodthinking.youngloduser.LoginActivity_Firebase"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.goodthinking.youngloduser.Upload_activity" />
        <activity android:name="com.goodthinking.youngloduser.MainActivity" />
        <activity android:name="com.goodthinking.youngloduser.ComingSoon" />
        <activity android:name="com.goodthinking.youngloduser.SignUpActivity_Firebase" />
        <activity android:name="com.goodthinking.youngloduser.NewsActivity" />
        <activity android:name="com.goodthinking.youngloduser.EventInformationActivity_Firebase" />
        <activity android:name="com.goodthinking.youngloduser.EventThanksActivity" />
        <activity android:name="com.goodthinking.youngloduser.MyEvent_Activity" />
        <activity android:name="com.goodthinking.youngloduser.EventRegisterationActivity" />
        <activity android:name="com.goodthinking.youngloduser.EventRecyclerview_Firebase" />
        <activity android:name="com.goodthinking.youngloduser.EventAddNew_Firebase" />
  

        <activity android:name="com.goodthinking.youngloduser.Applicant_list"></activity>
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />


        <activity android:name="com.goodthinking.youngloduser.AboutUsActivity" />


        <service android:name="com.goodthinking.youngloduser.MyService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity android:name="com.goodthinking.youngloduser.NewMessageToUsers"></activity>
    </application>

</manifest>